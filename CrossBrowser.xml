<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" >
 <listeners>
  		<listener class-name="utilities.ExtentReport" />
  </listeners>
  
  <test name="cromeBrowser Test">
  <parameter name="browser" value="chrome"/>
    <classes>
      <class name="testCases.HomePageTest"/>
      <class name="testCases.GlobalITTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test name="edgeBrowser Test">
    <parameter name="browser" value="edge"/>
    <classes>
      <class name="testCases.HomePageTest"/>
      <class name="testCases.GlobalITTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
